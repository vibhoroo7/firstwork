      "source": [
        "def isprime(num):\n",
        "    if num> 1:  \n",
        "        for n in range(2,num):  \n",
        "            if (num % n) == 0:  \n",
        "                return False\n",
        "            else:\n",
        "                return True\n",
        "    else:\n",
        "        return False"
      ],
